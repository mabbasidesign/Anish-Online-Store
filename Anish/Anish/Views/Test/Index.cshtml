@model IEnumerable<Anish.Models.Employee>

@{
    ViewBag.Title = "Index";
    Layout = null;
}

<h1> @ViewBag.Name </h1>

<link href="~/Content/bootstrap.css" rel="stylesheet" />

<table class="table table-hover">
    <tr>
        <td>Id</td>
        <td>Name</td>
        <td>Department</td>
    </tr>
    <tbody>
        @if (Model != null)
        {
            foreach (var item in Model)
            {
                <tr>
                    <td>@item.EmployeeId</td>
                    <td>@item.Name</td>
                    <td>@item.Department</td>
                </tr>
            }
        }
        
    </tbody>
</table>

