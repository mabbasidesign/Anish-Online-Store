@model Anish.Models.ModelC

@{
    ViewBag.Title = "IndexA";
}



<div class="panel panel-body">

    <div class="col-md-3">

        <div class="thumbnail">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <h3 class="panel-title">Director Message</h3>
                </div>
            </div>

            <img src="~/Content/logo.png" alt="..." style="margin-top:-20px">
            <div class="caption">
                <h3>Mohsen Abbasi</h3>
                <p>&para; Have faith in us, We will give you full satisfaction by providing best service. &para;</p>
                <p><a href="#" class="btn btn-primary" role="button">See more</a> </p>
            </div>
        </div>

    </div>

    <div class="col-md-9">
        <h3>@Model.Age</h3>

        @if (Model != null)
        {
            <div class="col-md-6">
                <ul class="list-group">
                    @foreach (var item in Model.ListA)
                     {
                        <li class="list-group-item">
                            @item.Name
                        </li>
                     }
                </ul>
            </div>

            <div class="col-md-6">
                <ul>
                    @foreach (var item in Model.ListB)
                     {
                        <li class="list-group-item">
                            @item.Country
                        </li>
                    }
                </ul>
            </div>

        }

    </div>
</div>






